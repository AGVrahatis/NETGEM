load HvsEtraceScore;
h = figure; 
plot(result.trH, result.sH, 'r*', 'MarkerSize', 10);
grid on; 
set(gca, 'FontSize', 15); 
xlabel('Trace');
ylabel('Change Score'); 
saveas(h, 'Qh', 'fig');
print(h, '-dpng', 'Qh'); 
close(h); 

h = figure; 
plot(result.trE, result.sE, 'r*', 'MarkerSize', 10);
grid on; 
set(gca, 'FontSize', 15); 
xlabel('Trace');
ylabel('Change Score'); 
saveas(h, 'Qe', 'fig');
print(h, '-dpng', 'Qe'); 